    "@heroicons/react": "^2.0.18",
    "@material-tailwind/react": "^2.1.1",
    "@vercel/analytics": "^1.0.2",
    "autoprefixer": "10.4.15",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "i18next": "^23.5.1",
    "next": "13.4.19",
    "next-i18next": "^14.0.3",
